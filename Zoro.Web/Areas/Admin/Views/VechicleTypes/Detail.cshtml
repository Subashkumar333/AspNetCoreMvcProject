@model VechicleTypes
<div class="container">
    <div class="Card">
        <div class="card-header text-center text-uppercase">
            <h2>cycleType Details</h2>
        </div>

        <div class="card-body">
            <div class="row">
                <div class="col-md-8">


                    <div class="row">

                        <div class="col-md-4">
                            <p class="card-text text-uppercase"><strong>Id</strong></p>
                        </div>
                        <div class="col-md-8">
                            <p class="card-text ">@Model.Id</p>
                        </div>

                    </div>

                    <div class="row">

                        <div class="col-md-4">
                            <p class="card-text text-uppercase"><strong>Name</strong></p>
                        </div>
                        <div class="col-md-8">
                            <p class="card-text ">@Model.Name</p>
                        </div>

                    </div>


                   


                </div>


                

            </div>
            <hr />
            <div class="row">
                <div class="col-md-12 text-center">
                    <a asp-area="Admin" asp-controller="VechicleTypes" asp-action="Index" class="btn btn-warning">Back</a>
                </div>
            </div>


        </div>
    </div>
</div>
